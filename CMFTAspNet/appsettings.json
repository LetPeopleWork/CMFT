{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AppContext": "Data Source=CMFTAppContext.db"
  },
  "PeriodicRefresh": {
    "Throughput": {
      "Interval": 60,
      "RefreshAfter": 360
    },
    "WorkItems": {
      "Interval": 60,
      "RefreshAfter": 360
    },
    "Forecasts": {
      "Interval": 20,
      "RefreshAfter": 120
    }
  }
}