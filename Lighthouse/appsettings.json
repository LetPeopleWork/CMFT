{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LighthouseAppContext": "Data Source=LighthouseAppContext.db"
  },
  "LighthouseVersion":  "DEV",
  "PeriodicRefresh": {
    "Throughput": {
      "Interval": 60,
      "RefreshAfter": 360,
      "StartDelay": 1
    },
    "WorkItems": {
      "Interval": 60,
      "RefreshAfter": 360,
      "StartDelay": 2
    },
    "Forecasts": {
      "Interval": 20,
      "RefreshAfter": 120,
      "StartDelay": 3
    }
  }
}