{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PeriodicRefresh": {
    "Throughput": {
      "Interval": 1,
      "RefreshAfter": 3,
      "StartDelay": 10
    },
    "WorkItems": {
      "Interval": 1,
      "RefreshAfter": 3,
      "StartDelay": 10
    },
    "Forecasts": {
      "Interval": 2,
      "RefreshAfter": 5,
      "StartDelay": 10
    }
  }
}
